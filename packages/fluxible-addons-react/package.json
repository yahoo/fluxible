{
  "name": "fluxible-addons-react",
  "version": "0.2.16",
  "description": "Fluxible addons for use with React",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/fluxible"
  },
  "scripts": {
    "dist": "../../node_modules/.bin/babel --root-mode upward src -d dist",
    "precover": "npm run dist",
    "prepublish": "npm run dist",
    "pretest": "npm run dist",
    "test": "../../node_modules/.bin/mocha tests/unit/",
    "cover": "BABEL_ENV=test ../../node_modules/.bin/nyc ../../node_modules/.bin/_mocha tests/unit/",
    "lint": "../../node_modules/.bin/eslint src/*.js tests/"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "fluxible": ">=1.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "author": "Michael Ridgway <mridgway@yahoo-inc.com>",
  "license": "BSD-3-Clause"
}
