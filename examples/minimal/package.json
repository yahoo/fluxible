    "fluxible": "^1.4.0",
    "fluxible-addons-react": "^1.0.0",
    "fluxible-reducer-store": "^0.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.7",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^3.0.0"
