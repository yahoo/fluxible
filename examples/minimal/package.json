{
  "name": "fluxible-minimal-example",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Pablo Palacios <pablo.palacios@holidaycheck.com>",
  "private": true,
  "scripts": {
    "build": "npm run build:browser && npm run build:server",
    "build:browser": "BABEL_ENV=browser webpack -c webpack.browser.config.js",
